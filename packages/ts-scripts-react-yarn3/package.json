    "@types/yargs": "^17.0.33",
    "@xylabs/ts-scripts-yarn3": "workspace:^",
    "@xylabs/tsconfig": "workspace:^",
    "publint": "^0.2.11",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=14.16"
  },
  "publishConfig": {
    "access": "public"
